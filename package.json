    "jquery": "^3.5.0",
    "laravel-mix": "^4.1.2",
    "pem": "^1.14.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reactstrap": "^8.0.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0"
  },
  "autoload": {
    "App": "./app"
  }
